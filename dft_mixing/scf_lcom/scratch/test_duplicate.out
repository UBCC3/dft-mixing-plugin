# Any line starting with the # character is a comment line
#! Sample HF/cc-pVDZ H2O computation

memory 600 mb   
import scf_lcom
import psi4

molecule h2o {
  O 
  H 1 0.96
  H 1 0.96 2 104.5
}

set basis cc-pVDZ

func1 = {
  "name": "func1",
  "coef": 1.0,
  "lcom_functionals":{
    "BLYP": 1.0
  }
}

func2 = {
  "name": "func2",
  "coef": 1.0,
  "lcom_functionals":{
    "BLYP": 1.0
  }
}

dft_functional1 = {
  "name": "test",
  "lcom_functionals":{
      "func1": func1, 
      "func2": func2,
 }
}



set scf_lcom {
  print 1
}

print(psi4.driver.procedures['energy']['scf_lcom'])
E1 = energy('scf_lcom', dft_functional=dft_functional1, decompose_xc=False)
print(psi4.variable('DFT XC ENERGY'))

psi4.core.clean()

E2 = energy('scf', dft_functional="BOP")
psi4.core.print_out(f"\nBOP ENERGY\n" )
print(psi4.variable('DFT XC ENERGY'))

psi4.core.clean()

E3 = energy('scf', dft_functional="BLYP")
psi4.core.print_out(f"\nBLYP ENERGY\n" )
print(psi4.variable('DFT XC ENERGY'))

psi4.core.clean()

dft_functional2 = {
  "name": "LCOM_REF",
  "x_functionals": {
      "GGA_X_B88": {
        "alpha": 1.0
      }
  },
  "c_functionals": {
      "GGA_C_OP_B88": {"alpha": 1.0},
      "GGA_C_LYP": {"alpha": 0.0}
  }
}

E4 = energy('scf', dft_functional=dft_functional2)
psi4.core.print_out(f"\nLCOM_REF\n" )
print(psi4.variable('DFT XC ENERGY'))



