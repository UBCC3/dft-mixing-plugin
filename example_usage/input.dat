# Any line starting with the # character is a comment line
#! Sample HF/cc-pVDZ H2O computation

memory 600 mb   
import scf_lcom
import psi4
import database
import os

path = os.path.join('/home/kenrickmh/CHEM/dft-mixing-plugin/example_usage/db_query_config.yaml') 
db = database.Psi4DbAdapter(path)

molecule h2o {
  O 
  H 1 0.96
  H 1 0.96 2 104.5
}

set basis cc-pVDZ
set scf_lcom {
  print 1
}

dft = db.get_functional_dict("multifunc1", "disp_mix1") 
print(dft)
E1 = energy('scf_lcom', dft_functional=dft, decompose_xc=True)
